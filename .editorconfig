# http://editorconfig.org

root = true

[*]
indent_style = space
trim_trailing_whitespace = true
indent_size = 4
end_of_line = CRLF

# Docstrings and comments use max_line_length = 79
[*.py]
max_line_length = 119

# The JSON files contain newlines inconsistently
[*.json]
indent_size = 2
insert_final_newline = ignore
